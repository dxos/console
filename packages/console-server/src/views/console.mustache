<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>{{ title }}</title>
  </head>
  <body>
    <div id="{{ container }}"></div>

    <!-- Config loaded by client. -->
    <script charset="utf-8" type="application/javascript">
      window.__DXOS__ = { config: {{{ config }}} };
    </script>

    <!-- React bundles. -->
    {{#scripts}}
      <script charset="utf-8" type="application/javascript" src="{{src}}"></script>
    {{/scripts}}
  </body>
</html>
