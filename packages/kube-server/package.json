    "debug": "^4.1.1",
    "express": "^4.17.1",
    "ipfs-http-client": "44.1.0",
    "js-yaml": "^4.1.0",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.pick": "^4.4.0",
    "node-cache": "^5.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "source-map-support": "^0.5.12",
    "systeminformation": "^4.26.5",
    "yargs": "^15.3.1"
