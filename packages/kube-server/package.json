{
  "name": "@dxos/kube-server",
  "version": "1.2.9-alpha.57",
  "description": "KUBE Server",
  "license": "AGPL-3.0",
  "main": "dist/es/index.js",
  "bin": {
    "dxos-console": "bin/kube-server.js",
    "kube-server": "bin/kube-server.js"
  },
  "files": [
    "bin/",
    "dist/es"
  ],
  "scripts": {
    "build": "npm run clean && babel ./src --out-dir ./dist/es --ignore \"**/*.test.js\" --copy-files",
    "ci": "npm run build && npm run lint && npm run test",
    "clean": "rm -rf ./dist",
    "lint": "semistandard 'src/**/*.js'",
    "start": "CONFIG_FILE=${CONFIG_FILE=~/.wire/profile/default} BABEL_DISABLE_CACHE=1 nodemon --exec babel-node src/server.js -- --verbose",
    "test": "jest --rootDir ./src --passWithNoTests --no-cache"
  },
  "eslintConfig": {
    "extends": "plugin:@dxos/react"
  },
  "jest": {
    "preset": "ts-jest/presets/default",
    "testEnvironment": "node"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@babel/runtime": "^7.8.7",
    "@dxos/config": "~2.10.2",
    "apollo-boost": "^0.4.9",
    "apollo-server-express": "^2.13.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^6.0.3",
    "ipfs-http-client": "44.1.0",
    "js-yaml": "^3.14.0",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.pick": "^4.4.0",
    "node-cache": "^5.1.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "source-map-support": "^0.5.12",
    "systeminformation": "^4.26.5",
    "tree-kill": "^1.2.2",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@dxos/eslint-plugin": "^1.0.14",
    "babel-jest": "^27.2.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-import-graphql": "^2.7.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-inline-json-import": "^0.3.2",
    "eslint": "^7.10.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^24.8.0",
    "nodemon": "^2.0.4",
    "semistandard": "^14.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
