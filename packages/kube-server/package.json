{
  "name": "@dxos/kube-server",
  "version": "1.2.9-alpha.66",
  "description": "KUBE Server",
  "license": "AGPL-3.0",
  "main": "dist/src/server.js",
  "bin": {
    "dxos-console": "bin/kube-server.js",
    "kube-server": "bin/kube-server.js"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "tsc && cp -R ./*.yml dist",
    "clean": "rm -rf ./dist",
